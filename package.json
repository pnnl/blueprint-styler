{
  "name": "blueprint-styler",
  "version": "0.0.1",
  "description": "Make custom blueprint.css styles",
  "author": "James Bradford",
  "repository": {
    "type": "git",
    "url": "https://stash.pnnl.gov/projects/UXRSRC/repos/blueprint-styler"
  },
  "scripts": {
    "start": "yarn copy",
    "copy:clean": "rm -r src",
    "copy:scss": "copyfiles --all --verbose --up 1 -e './**/lib/**/*' -e './**/docs-theme/**/*' -e './**/monorepo/**/*' './node_modules/@blueprintjs/**/*.scss' './src/styles/_blueprint' && echo '> DONE copying scss'",
    "copy:examples": "copyfiles --all --verbose --up 6 './node_modules/@blueprintjs/monorepo/packages/docs-app/src/examples/**/*' './node_modules/@blueprintjs/monorepo/packages/docs-app/src/common/**/*' './src' && echo '> DONE copying examples'",
    "copy": "yarn copy:clean && yarn copy:scss && yarn copy:examples && echo '> DONE copying all files'"
  },
  "devDependencies": {
    "@blueprintjs/core": "^4.14.1",
    "@blueprintjs/datetime": "^4.4.10",
    "@blueprintjs/datetime2": "^0.9.8",
    "@blueprintjs/docs-theme": "^4.4.22",
    "@blueprintjs/icons": "^4.12.1",
    "@blueprintjs/popover2": "^1.11.1",
    "@blueprintjs/select": "^4.8.14",
    "@blueprintjs/table": "^4.7.12",
    "@blueprintjs/timezone": "^4.5.14",
    "@blueprintjs/monorepo": "palantir/blueprint#develop",
    "copyfiles": "^2.4.1"
  }
}
