{
  "name": "blueprint-styler",
  "version": "0.0.1",
  "description": "Make custom blueprint.css styles",
  "author": "James Bradford",
  "repository": {
    "type": "git",
    "url": "https://stash.pnnl.gov/projects/UXRSRC/repos/blueprint-styler"
  },
  "scripts": {
    "start": "yarn copy",
    "copy:scss": "copyfiles --all --verbose --up 1 -e './**/lib/**/*' -e './**/docs-theme/**/*' './node_modules/@blueprintjs/**/*.scss' './src/styles/_default-var-styles' && echo '> DONE copying scss'",
    "copy:examples": "copyfiles --all --verbose --up 5 '../blueprint/packages/docs-app/src/examples/**/*' './src' && echo '> DONE copying examples'",
    "copy": "npm run copy:scss && npm run copy:examples && echo '> DONE copying all files'"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.46.0",
    "@blueprintjs/datetime": "^3.23.5",
    "@blueprintjs/docs-theme": "^3.10.5",
    "@blueprintjs/icons": "^3.27.0",
    "@blueprintjs/popover2": "^0.11.0",
    "@blueprintjs/select": "^3.16.5",
    "@blueprintjs/table": "^3.8.34",
    "@blueprintjs/timezone": "^3.9.5",
    "copyfiles": "^2.4.1"
  }
}
