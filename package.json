{
  "name": "@blueprintjs/docs-app",
  "version": "3.24.0",
  "description": "Blueprint Documentation Site",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "postinstall": "npm run generate-svg-icon-files",
    "bundle": "webpack --config ./webpack.config.js",
    "clean": "rm -rf dist/* lib/*",
    "dev": "webpack-dev-server --config ./webpack.config.js --host 0.0.0.0",
    "build": "run-p build:lib build:dist",
    "build:dist": "cross-env NODE_ENV=production yarn bundle",
    "build:lib": "npm run gulp",
    "gulp": "gulp",
    "lint": "run-p lint:scss lint:es",
    "lint:scss": "sass-lint",
    "lint:es": "es-lint",
    "lint-fix": "es-lint --fix",
    "test": "exit 0",
    "verify": "run-p dist lint",
    "generate-svg-icon-files": "node ./scripts/generate-svg-icon-files.js"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.30.1",
    "@blueprintjs/datetime": "^3.18.5",
    "@blueprintjs/docs-theme": "^3.6.4",
    "@blueprintjs/icons": "^3.20.0",
    "@blueprintjs/select": "^3.13.6",
    "@blueprintjs/table": "^3.8.12",
    "@blueprintjs/test-commons": "^0.10.4",
    "@blueprintjs/timezone": "^3.6.10",
    "@types/chai": "4.2.12",
    "@types/classnames": "2.2.10",
    "@types/downloadjs": "^1.4.2",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/mocha": "8.0.1",
    "@types/prop-types": "15.7.3",
    "@types/react": "16.9.44",
    "@types/react-dom": "16.9.8",
    "@types/react-lifecycles-compat": "^3.0.1",
    "@types/react-transition-group": "4.4.0",
    "@types/sinon": "9.0.4",
    "@types/webpack": "4.41.21",
    "chai": "^4.2.0",
    "chroma-js": "^2.1.0",
    "circle-github-bot": "^2.1.0",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "dom4": "^2.1.5",
    "downloadjs": "^1.4.7",
    "gh-pages": "^3.1.0",
    "gulp-sass": "^4.1.0",
    "http-server": "^0.12.3",
    "lerna": "^3.22.1",
    "moment": "^2.27.0",
    "node-sass": "^4.14.1",
    "node-sass-tilde-importer": "^1.0.2",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.16.1",
    "postcss-combine-duplicated-selectors": "^9.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-transition-group": "^4.4.1",
    "sass-inline-svg": "^1.2.3",
    "sinon": "^9.0.2",
    "stylelint-config-palantir": "^5.0.0",
    "stylelint-scss": "^3.18.0",
    "tslib": "~2.0.0",
    "typescript": "~3.9.7",
    "yarn-deduplicate": "^2.1.1"
  },
  "devDependencies": {
    "@blueprintjs/node-build-scripts": "^1.3.2",
    "@blueprintjs/webpack-build-scripts": "^1.0.1",
    "@types/chroma-js": "^2.0.0",
    "gulp": "^4.0.2",
    "gulp-cssbeautify": "^2.0.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
