{
  "name": "blueprintjs-custom-theme",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "npm run generate-svg-icon-files",
    "generate-svg-icon-files": "node ./scripts/generate-svg-icon-files.js",
    "compile:css": "node-sass-chokidar ./styles --output src/themes --source-map true --include-path ./node_modules/@blueprintjs/core/styles/ --importer ./node_modules/node-sass-package-importer/dist/cli.js --functions ./scripts/sass-custom-functions.js",
    "dev": "run-p \"compile:esm -- --watch\" \"compile:css -- --watch\"",
    "dist:css": "css-dist lib/css/*.css",
    "dist:variables": "generate-css-variables common/_colors.scss common/_color-aliases.scss common/_variables.scss",
    "lint:scss": "sass-lint"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "@blueprintjs/core": "^3.22.3",
    "@blueprintjs/icons": "^3.12.0",
    "node-sass-chokidar": "^1.4.0",
    "node-sass-package-importer": "^5.3.2",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "sass-inline-svg": "^1.2.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}