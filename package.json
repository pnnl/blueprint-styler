{
  "name": "blueprint-styler",
  "version": "3.1.0-alpha.1",
  "description": "Make custom blueprint.css styles",
  "author": "James Bradford",
  "repository": {
    "type": "git",
    "url": "https://stash.pnnl.gov/projects/UXRSRC/repos/blueprint-styler"
  },
  "publishConfig": {
    "registry": "https://npm.pnnl.gov/artifactory/api/npm/npm"
  },
  "main": "./dist/index.html",
  "files": [
    "/lib",
    "/src/styles"
  ],
  "keywords": [
    "palantir",
    "blueprint",
    "css",
    "styles"
  ],
  "scripts": {
    "start": "npm run dev",
    "bundle": "webpack --config ./webpack.config.js",
    "clean": "rm -rf dist/* lib/*",
    "dev": "webpack-dev-server --config ./webpack.config.js --host 0.0.0.0",
    "build": "npm run build:dist && npm run build:lib",
    "build:icons": "npm run generate-svg-icon-files",
    "build:dist": "cross-env NODE_ENV=production yarn bundle",
    "build:lib": "npm run gulp",
    "deploy": "npm publish --registry https://npm.pnnl.gov/artifactory/api/npm/npm",
    "version:alpha": "npm version prerelease --preid alpha",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "merge-copy": "git merge development-copy-only",
    "gulp": "gulp",
    "lint": "run-p lint:scss lint:es",
    "lint:scss": "sass-lint",
    "lint:es": "es-lint",
    "lint-fix": "es-lint --fix",
    "test": "exit 0",
    "verify": "run-p dist lint",
    "generate-svg-icon-files": "node ./scripts/generate-svg-icon-files.js"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.44.0",
    "@blueprintjs/datetime": "^3.23.0",
    "@blueprintjs/docs-theme": "^3.10.0",
    "@blueprintjs/icons": "^3.26.0",
    "@blueprintjs/popover2": "^0.6.0",
    "@blueprintjs/select": "^3.16.0",
    "@blueprintjs/table": "^3.8.29",
    "@blueprintjs/timezone": "^3.9.0",
    "@carbon/colors": "^10.23.0",
    "@ibm/plex": "^5.1.3",
    "@types/chai": "4.2.15",
    "@types/classnames": "2.2.11",
    "@types/downloadjs": "^1.4.2",
    "@types/enzyme": "3.10.8",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/mocha": "8.2.2",
    "@types/prop-types": "15.7.3",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.2",
    "@types/react-lifecycles-compat": "^3.0.1",
    "@types/react-transition-group": "4.4.1",
    "@types/sinon": "9.0.11",
    "@types/webpack": "^3.8.20",
    "chai": "^4.3.4",
    "chroma-js": "^2.1.1",
    "circle-github-bot": "^2.1.0",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^6.4.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "dom4": "^2.1.6",
    "downloadjs": "^1.4.7",
    "gh-pages": "^3.1.0",
    "gulp-sass": "^4.1.0",
    "http-server": "^0.12.3",
    "lerna": "^3.22.1",
    "lodash-es": "^4.17.21",
    "merge-stream": "^2.0.0",
    "moment": "^2.29.1",
    "node-sass": "^4.5.3",
    "node-sass-tilde-importer": "^1.0.2",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.16.1",
    "postcss": "^8.2.8",
    "postcss-combine-duplicated-selectors": "^10.0.2",
    "postcss-custom-properties": "^11.0.0",
    "postcss-remove-null": "^1.2.1",
    "postcss-remove-root": "^0.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-transition-group": "^4.4.1",
    "sass-inline-svg": "^1.2.3",
    "sinon": "^9.2.4",
    "stylelint-config-palantir": "^5.0.0",
    "stylelint-scss": "^3.19.0",
    "tslib": "~2.1.0",
    "typescript": "^3.9.9",
    "yarn-deduplicate": "^3.1.0"
  },
  "devDependencies": {
    "@blueprintjs/node-build-scripts": "^1.5.0",
    "@blueprintjs/webpack-build-scripts": "^1.2.0",
    "@types/chroma-js": "^2.1.3",
    "gulp": "^4.0.2",
    "gulp-cssbeautify": "^3.0.0",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "through2": "^4.0.2",
    "vinyl": "^2.2.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.1"
  }
}
