    "@blueprintjs/popover2": "^0.12.4",
    "@blueprintjs/select": "^3.18.6",
    "@blueprintjs/table": "^3.9.8",
    "@blueprintjs/timezone": "^3.9.14"
  },
  "devDependencies": {
    "@blueprintjs/core": "^3.50.4",
    "@blueprintjs/datetime": "^3.23.14",
    "@blueprintjs/docs-theme": "^3.10.14",
    "@blueprintjs/icons": "^3.30.2",
