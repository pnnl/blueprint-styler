    "@blueprintjs/timezone": "^3.9.14",
    "@carbon/colors": "^10.33.0",
    "@ibm/plex": "^6.0.0",
    "@types/node": "^16.10.2",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "copyfiles": "^2.4.1",
    "cssnano": "^5.0.8",
    "gulp": "^4.0.2",
    "gulp-cssbeautify": "^3.0.0",
    "gulp-postcss": "^9.0.1",
