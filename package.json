{
  "name": "blueprint-styler",
  "version": "3.1.0-alpha.7",
  "description": "Make custom blueprint.css styles",
  "author": "James Bradford",
  "repository": {
    "type": "git",
    "url": "https://stash.pnnl.gov/projects/UXRSRC/repos/blueprint-styler"
  },
  "homepage": ".",
  "publishConfig": {
    "registry": "https://npm.pnnl.gov/artifactory/api/npm/npm"
  },
  "main": "./dist/index.html",
  "files": [
    "/lib",
    "/src/styles/**/*.scss"
  ],
  "keywords": [
    "palantir",
    "blueprint",
    "css",
    "styles"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn run build:react && yarn run build:libs",
    "build:react": "react-scripts build",
    "build:libs": "yarn run gulp",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "npm publish --registry https://npm.pnnl.gov/artifactory/api/npm/npm",
    "version:alpha": "npm version prerelease --preid alpha",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "merge-copy": "git merge development-copy-only",
    "gulp": "gulp",
    "generate-svg-icon-files": "node ./scripts/generate-svg-icon-files.js",
    "copy:examples": "copyfiles --all --verbose --up 5 '../blueprint/packages/docs-app/src/examples/**/*' './src' && echo '> DONE copying examples'"
  },
  "peerDependencies": {
    "@blueprintjs/core": "^3.44.2",
    "@blueprintjs/datetime": "^3.23.2",
    "@blueprintjs/docs-theme": "^3.10.2",
    "@blueprintjs/icons": "^3.26.0",
    "@blueprintjs/popover2": "^0.8.0",
    "@blueprintjs/select": "^3.16.2",
    "@blueprintjs/table": "^3.8.31",
    "@blueprintjs/timezone": "^3.9.2"
  },
  "devDependencies": {
    "@blueprintjs/core": "^3.44.2",
    "@blueprintjs/datetime": "^3.23.2",
    "@blueprintjs/docs-theme": "^3.10.2",
    "@blueprintjs/icons": "^3.26.0",
    "@blueprintjs/popover2": "^0.8.0",
    "@blueprintjs/select": "^3.16.2",
    "@blueprintjs/table": "^3.8.31",
    "@blueprintjs/timezone": "^3.9.2",
    "@carbon/colors": "^10.25.0",
    "@ibm/plex": "^5.1.3",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "copyfiles": "^2.4.1",
    "lodash-es": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.2.4",
    "web-vitals": "^1.1.2",
    "cssnano": "^5.0.4",
    "gulp": "^4.0.2",
    "gulp-cssbeautify": "^3.0.0",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "merge-stream": "^2.0.0",
    "node-sass": "^6.0.0",
    "node-sass-tilde-importer": "^1.0.2",
    "postcss": "^8.3.0",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-custom-properties": "^11.0.0",
    "postcss-remove-null": "^1.2.1",
    "postcss-remove-root": "^0.0.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
